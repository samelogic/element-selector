{
    "manifest_version": 2,
    "name": "CSS Path Selector",
    "version": "1.0",
    "description": "Get CSS Path of any element",
    "permissions": ["activeTab"],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"]
        }
    ]
}
